{
  "name": "@panyam/tsutils-ui",
  "version": "0.0.2",
  "author": "Sriram Panyam",
  "description": "Helper utilities to deal with ui and layouts",
  "homepage": "https://github.com/panyam/tsutils-ui#readme",
  "license": "ISC",
  "types": "lib/cjs/index.d.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panyam/tsutils-ui.git"
  },
  "keywords": [
    "tsutils-ui"
  ],
  "bugs": {
    "url": "https://github.com/panyam/tsutils-ui/issues"
  },
  "pre-commit": [
    "precommit-msg",
    "clean",
    "lintfix",
    "test"
  ],
  "scripts": {
    "clean": "rm -Rf dist && jest --clearCache && rm -rf static/dist",
    "test": "jest --coverage --runInBand",
    "start": "webpack-dev-server --mode=development",
    "build": "tsc",
    "lint": "eslint './src' --quiet --ext .ts --resolve-plugins-relative-to .",
    "lintfix": "eslint 'src/**/*.ts' --quiet --fix --resolve-plugins-relative-to .",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run lintfix && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@panyam/tsutils": "^0.0.14",
    "@types/handlebars": "^4.1.0",
    "@types/node": "^14.14.26",
    "@typescript-eslint/parser": "^4.15.0",
    "axios": "^0.21.1",
    "js-yaml": "^4.0.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-typescript": "^7.12.16",
    "@types/fs-extra": "^9.0.7",
    "@types/jest": "^26.0.20",
    "@types/js-yaml": "^4.0.0",
    "@types/jsdom": "^16.2.6",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.8.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "jest": "^26.6.3",
    "jest-each": "^26.6.2",
    "jest-mock": "^26.6.2",
    "jsdom": "^16.4.0",
    "json5": "^2.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.24",
    "typescript": "^4.1.5",
    "typescript-tslint-plugin": "^1.0.1"
  }
}
