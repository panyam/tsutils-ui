{
  "name": "@panyam/tsutils-ui",
  "version": "0.0.16",
  "author": "Sriram Panyam",
  "description": "Helper utilities to deal with ui and layouts",
  "homepage": "https://github.com/panyam/tsutils-ui#readme",
  "license": "ISC",
  "types": "lib/cjs/index.d.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/",
    "styles/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panyam/tsutils-ui.git"
  },
  "keywords": [
    "tsutils-ui"
  ],
  "bugs": {
    "url": "https://github.com/panyam/tsutils-ui/issues"
  },
  "pre-commit": [
    "precommit-msg",
    "clean",
    "lintfix",
    "test"
  ],
  "scripts": {
    "clean": "rm -Rf lib && jest --clearCache && rm -rf static/lib",
    "build": "npm run  clean && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "test": "jest --coverage --runInBand",
    "start": "webpack-dev-server --mode=development",
    "lint": "eslint './src' --quiet --ext .ts --resolve-plugins-relative-to .",
    "lintfix": "eslint 'src/**/*.ts' --quiet --fix --resolve-plugins-relative-to .",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run build && npm run lint",
    "version": "npm run lintfix && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@panyam/tsutils": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-flow": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/js-yaml": "^4.0.1",
    "@types/jsdom": "^16.2.12",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.8.2",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.1",
    "jest": "^27.0.6",
    "jest-each": "^27.0.6",
    "jest-mock": "^27.0.6",
    "jsdom": "^16.6.0",
    "json5": "^2.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typedoc": "^0.21.2",
    "typescript": "^4.3.5",
    "typescript-tslint-plugin": "^1.0.1"
  }
}
